<h1> Users </h1>
<table border="1" >
  <tr>
    <th>Name</th>
    <th>Cellphone</th>
    <th>Email</th>
  </tr>
  <% @users.each do | user | %>
   <tr>
     <td><%= user.first_name %> <%= user.last_name %></td>
     <td><%= user.cellphone %></td>
     <td><%= user.email %></td>
   </tr>
  <% end %>
</table>